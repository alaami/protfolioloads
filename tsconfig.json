{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "preserve",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@/app/*": ["src/app/*"],
      "@/page/*": ["src/page/*"],
      "@/main/*": ["src/main/*"]
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "next.config.js", "src/functions/preBuildScripts/preBuildDev.js","src/functions/preBuildScripts/preBuildUtilities.js" ],
  "exclude": [
    "node_modules"
  ]
}
